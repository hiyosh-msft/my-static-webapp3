{
    "routes": [
        {
            "route": "/index2.html",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/login",
            "redirect": "/.auth/login/aad"
        },
        {
             "route": "/logout",
             "redirect": "/.auth/logout"
        }
    ],
    
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    
    "responseOverrides": {
        "401": {
          "redirect": "/login?post_login_redirect_uri=.referrer",
          "statusCode": 302
        }
    }
    
}
